!!! 5
html(lang='en')
  head

    block head

      //- Standard Meta Tags
      meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
      meta(charset='utf-8')
      meta(name='description', content=properties.description)
      meta(name='keywords', content=properties.keywords)
      meta(name='author', content='ctrl')

      //- Page Title
      title=page.title + ' - ' + properties.name

      link(rel='shortcut icon', href=versionPath('/admin/images/structure/favicon.ico'))

      script(src=versionPath('/admin/js/lib/modernizr.custom.54727.js'))

    block styles
      //- CSS
      link(rel='stylesheet', href=versionPath('/admin/css/index.css'))
      link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Lato:400|Montserrat:700')

  body(class=page.section)

    .container
      block header
        include includes/structure/main-header

      .main-content
        block content

      block footer
        include includes/structure/main-footer

    block script
      script(src='//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js')
      script
        if (typeof jQuery === 'undefined') {
          document.write('<script src="/js/jquery-1.7.2.min.js" />');
        }
      script(src='//ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.min.js')
      script
        if (typeof jQuery.ui === 'undefined') {
          document.write('<script src="/js/jquery-ui-1.8.16.min.js" />');
        }

      - if (typeof compactJsHtml !== 'undefined')
        !=compactJsHtml()